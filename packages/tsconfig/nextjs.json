{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "Next.js",
  "compilerOptions": {
    "target": "es5",
    "noEmit": true,
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "incremental": true,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
